{
  "name": "json-id-transformer",
  "version": "0.0.1",
  "description": "A flexible utility library designed to batch transform specific IDs within a JSON object.",
  "keywords": [
    "json",
    "id",
    "transformer",
    "utility",
    "library",
    "jsonpath",
    "batch"
  ],
  "homepage": "https://github.com/tonyfromundefined/json-id-transformer#readme",
  "bugs": {
    "url": "https://github.com/tonyfromundefined/json-id-transformer/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tonyfromundefined/json-id-transformer.git"
  },
  "license": "MIT",
  "author": "Tony <jeehyukwon@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bunchee",
    "release": "changeset publish",
    "test": "vitest run"
  },
  "dependencies": {
    "json-pointer": "^0.6.2",
    "jsonpath-plus": "^10.3.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@changesets/cli": "^2.28.1",
    "@types/json-pointer": "^1.0.34",
    "bunchee": "^6.4.0",
    "typescript": "^5.8.2",
    "vitest": "^3.0.7"
  },
  "packageManager": "yarn@4.9.2",
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
